        wellLoc4.add("1331");wellRate4.add(5.0);
        wellLoc4.add("1330");wellRate4.add(7.0);
        //endregion

        //region Main Gate wells and ArrayList
        ArrayList<String> wellLocMain = new ArrayList<>();
        ArrayList<Double> wellRateMain = new ArrayList<>();
        wellLocMain.add("1212"); wellRateMain.add(5.0);
        wellLocMain.add("1111"); wellRateMain.add(7.0);
        wellLocMain.add("2138"); wellRateMain.add(7.0);
        wellLocMain.add("2145 WCI"); wellRateMain.add(9.0);
        wellLocMain.add("2145 EB"); wellRateMain.add(4.0);
        wellLocMain.add("2012"); wellRateMain.add(8.0);
        wellLocMain.add("2762"); wellRateMain.add(21.0);//position 6

        //Main Gate Part 2
        wellLocMain.add("2775"); wellRateMain.add(14.0);//7
        wellLocMain.add("3824"); wellRateMain.add(5.0);
        wellLocMain.add("2317"); wellRateMain.add(2.0);
        wellLocMain.add("2729"); wellRateMain.add(6.0);
        wellLocMain.add("2745"); wellRateMain.add(9.0); //position 11

        //Main Gate Part 3
        wellLocMain.add("2794"); wellRateMain.add(4.0);//12
        wellLocMain.add("2793"); wellRateMain.add(8.0);//13
        wellLocMain.add("2795"); wellRateMain.add(8.0);//position 14
        wellLocMain.add("1837"); wellRateMain.add(18.0); // Outside the main gate
        wellLocMain.add("3909"); wellRateMain.add(4.0); //Outside the main gate


        //endregion

        //region Gate 2
        ArrayList<String> wellLoc2 = new ArrayList<>(); //Array for the wells
        ArrayList<Double> wellRate2 = new ArrayList<>(); //Array for the rates
        wellLoc2.add("2710"); wellRate2.add(5.0);
        wellLoc2.add("2904"); wellRate2.add(6.0);
        wellLoc2.add("2749"); wellRate2.add(3.0);
        wellLoc2.add("5026"); wellRate2.add(4.0);
        //endregion

        //region Meter Sites
        ArrayList<String> wellMeterLoc = new ArrayList<>();
        ArrayList<Double> wellMeterRate = new ArrayList<>();
        wellMeterLoc.add("MS13 WGS"); wellMeterRate.add(1.0);
        wellMeterLoc.add("MS13 WCI"); wellMeterRate.add(39.0);
        wellMeterLoc.add("Comp St EB"); wellMeterRate.add(11.0);
        wellMeterLoc.add("Comp St WGS"); wellMeterRate.add(38.0);
        wellMeterLoc.add("MS 23 EB"); wellMeterRate.add(3.0);
        wellMeterLoc.add("MS 23 WCI"); wellMeterRate.add(37.0);
        wellMeterLoc.add("MS 26 EB"); wellMeterRate.add(2.0);
        wellMeterLoc.add("MS 24 EB"); wellMeterRate.add(3.0);
        //endregion

        //region Well names in cell order
        Row dataRow = sheet.createRow(1);//Starting at row 2 since row 1 is the titles
        //region Gate 3 wells (Printed on Sheet even if no inventory taken)
        Cell dataCell = dataRow.createCell(0);dataCell.setCellValue("178");
        dataRow = sheet.createRow(2);dataCell = dataRow.createCell(0);
        dataCell.setCellValue("186");dataRow = sheet.createRow(3);
        dataCell = dataRow.createCell(0);dataCell.setCellValue("167");
        //endregion

        //region Gate 4 wells (Printed on Sheet even if no inventory taken)
        dataRow = sheet.createRow(4);dataCell = dataRow.createCell(0);dataCell.setCellValue("1831");
        dataRow = sheet.createRow(5);dataCell = dataRow.createCell(0);dataCell.setCellValue("1835");
        dataRow = sheet.createRow(6);dataCell = dataRow.createCell(0);dataCell.setCellValue("1830");
        dataRow = sheet.createRow(7);dataCell = dataRow.createCell(0);dataCell.setCellValue("1401");
        dataRow = sheet.createRow(8);dataCell = dataRow.createCell(0);dataCell.setCellValue("1331");
        dataRow = sheet.createRow(9);dataCell = dataRow.createCell(0);dataCell.setCellValue("1330");
        //endregion

        //region Main Gate wells (Printed on sheet even if no inventory taken)
        dataRow = sheet.createRow(10); dataCell = dataRow.createCell(0); dataCell.setCellValue("1212");
        dataRow = sheet.createRow(11); dataCell = dataRow.createCell(0); dataCell.setCellValue("1111");
        dataRow = sheet.createRow(12); dataCell = dataRow.createCell(0); dataCell.setCellValue("2138");
        dataRow = sheet.createRow(13); dataCell = dataRow.createCell(0); dataCell.setCellValue("2145 WCI");
        dataRow = sheet.createRow(14); dataCell = dataRow.createCell(0); dataCell.setCellValue("2145 EB");
        dataRow = sheet.createRow(15); dataCell = dataRow.createCell(0); dataCell.setCellValue("2012");
        dataRow = sheet.createRow(16); dataCell = dataRow.createCell(0); dataCell.setCellValue("2762");

        //part 2 Main Gate
        dataRow = sheet.createRow(17); dataCell = dataRow.createCell(0); dataCell.setCellValue("2775");
        dataRow = sheet.createRow(18); dataCell = dataRow.createCell(0); dataCell.setCellValue("3824");
        dataRow = sheet.createRow(19); dataCell = dataRow.createCell(0); dataCell.setCellValue("2317");
        dataRow = sheet.createRow(20); dataCell = dataRow.createCell(0); dataCell.setCellValue("2729");
        dataRow = sheet.createRow(21); dataCell = dataRow.createCell(0); dataCell.setCellValue("2745");


        //endregion

        //region Gate 2 wells (Printed on sheet even of no inventory taken)
        dataRow = sheet.createRow(22); dataCell = dataRow.createCell(0); dataCell.setCellValue("2710");
        dataRow = sheet.createRow(23); dataCell = dataRow.createCell(0); dataCell.setCellValue("2904");
        dataRow = sheet.createRow(24); dataCell = dataRow.createCell(0); dataCell.setCellValue("2749");
        dataRow = sheet.createRow(25); dataCell = dataRow.createCell(0); dataCell.setCellValue("5026");
        //endregion

        //region Main Gate Part 3 wells (2794,2793,2795,1837,3909)
        dataRow = sheet.createRow(26); dataCell = dataRow.createCell(0); dataCell.setCellValue("2794");
        dataRow = sheet.createRow(27); dataCell = dataRow.createCell(0); dataCell.setCellValue("2793");
        dataRow = sheet.createRow(28); dataCell = dataRow.createCell(0); dataCell.setCellValue("2795");
