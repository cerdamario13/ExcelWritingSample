        //Creating the Scanner for user input
        Scanner console = new Scanner(System.in);

        int count1 = 1; //Counting for the Deliveries worksheet
        //While loop will allow to repeatedly input values until 'stop' is typed
        String well = "prime"; //priming the loop
        //MAIN while loop
        while (!well.equals("STOP")) {
            //Asking for the well name/number
            System.out.print("Well: ");
            String well1 = console.next();
            well = well1.toUpperCase();

            //adding a break to stop the loop when 'stop' is typed
            if (well.equals("STOP")) {
                break;
            }
            //region start if statement FOR ALL WELLS
            else if (!well.equals("178") && !well.equals("186") && !well.equals("167") &&
                    //Gate 4 (1831, 1835, 1830, 1401, 1331, 1330)
                    !well.equals("1831") && !well.equals("1835") && !well.equals("1830") &&
                    !well.equals("1401") && !well.equals("1331") && !well.equals("1330") &&
                    //Main Gate (1212,1111,2138,2012,2762,2145EB,2145WCI)
                    !well.equals("1212") && !well.equals("1111") && !well.equals("2138") &&
                    !well.equals("2012") && !well.equals("2762") && !well.equals("2145EB") &&
                    !well.equals("2145eb") && !well.equals("2145WCI") && !well.equals("2145wci") &&
                    //Main gate 2
                    !well.equals("2775") && !well.equals("3824") && !well.equals("2317") && !well.equals("2729") &&
                    !well.equals("2745") &&
                    //Main Gate part 3
                    !well.equals("2794") && !well.equals("2793") && !well.equals("2795") && !well.equals("1837") &&
                    !well.equals("3909") &&
                    //Gate 2
                    !well.equals("2710") && !well.equals("2904") && !well.equals("2749") && !well.equals("5026")

            ) {
                //If well entered is not valid, the loop will restart and ask the user again
                System.out.println("Well not valid. Try again.");
                continue; //Restart the loop
            }
            //endregion

            //Asking the user to input the well inventory and rate (Before the main if statement)
            System.out.print("Inventory: ");
            Double inv = console.nextDouble();
            //System.out.print("Current rate: ");
            //Double rate = console.nextDouble();

            //region metal tank dimensions. For wells that have metal tank only. see method below
            double tLength = 36;
            double tDiam = 30;
            double tRad = tDiam / 2;
            //endregion

            //region GATE 3 MAIN if statement (WELLS 178, 186, 167)
            if (well1.equals("178")) { //INDEX 0
                double Rate178 = wellRate3.get(0);//**INCREMENTS**//
                String Well178 = wellLoc3.get(0);//**INCREMENTS**//
                double daysLeft = Math.round((inv - 35) / Rate178);
                //Creating the rows where the data will be entered
                dataRow = sheet.createRow(1); //Row number 2 THIS WILL INCREMENT
                dataCell = dataRow.createCell(0); //starting in column 1
                dataCell.setCellValue(Well178);
                //Next Column
                dataCell = dataRow.createCell(1);//Column 2;
                dataCell.setCellValue(Rate178);
                //Next Column
                dataCell = dataRow.createCell(2);//Column 3
                dataCell.setCellValue(inv);
                //Next Column
                dataCell = dataRow.createCell(3);//Column 4
                dataCell.setCellValue(daysLeft);

                //**Deliveries Sheet**//
                if (inv <= 35.0) {
                    //count1++;
                    Row dataRow2 = sheet2.createRow(count1++); //Row number for the deliveries INCREMENT
                    Cell dataCell2 = dataRow2.createCell(0);//Starting in column 1
                    dataCell2.setCellValue(Well178); //Printing the name of well
                    //Next Column
                    if (((Rate178 / 4) * 30) < 55) { //**INCREMENTS**//
                        dataCell2 = dataRow2.createCell(1);//Column 2
                        dataCell2.setCellValue("55");//Delivering 55
                    }//Inside if
                    else if ((Rate178 / 4) * 30 > 56) {
                        dataCell2 = dataRow2.createCell(1);//Column 2
                        dataCell2.setCellValue("110"); //Delivering 110
                    }//Inner else if
                } //Outer if

            } else if (well.equals("186")) { //INDEX 1
                double Rate186 = wellRate3.get(1);//**INCREMENTS**//
                String Well186 = wellLoc3.get(1);//**INCREMENTS**//
                double daysLeft = Math.round((inv - 35) / Rate186);
                //Creating the rows where the data will be entered
                dataRow = sheet.createRow(2); //Row number 3 **THIS WILL INCREMENT**
                dataCell = dataRow.createCell(0); //starting in column 1
                dataCell.setCellValue(Well186);
                //Next Column
                dataCell = dataRow.createCell(1);//Column 2;
                dataCell.setCellValue(Rate186);
                //Next Column
                dataCell = dataRow.createCell(2);//Column 3
                dataCell.setCellValue(inv);
                //Next Column
                dataCell = dataRow.createCell(3);//Column 4
                dataCell.setCellValue(daysLeft);

                //**Deliveries Sheet**//
                if (inv <= 35.0) {
                    Row dataRow2 = sheet2.createRow(count1++); //Row number for the deliveries INCREMENT
                    Cell dataCell2 = dataRow2.createCell(0);//Starting in column 1
                    dataCell2.setCellValue(Well186);//**INCREMENTS**//
                    //Next Column
