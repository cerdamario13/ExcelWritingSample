    //region tank equation method
    public static double tankEq(double inv, double tLength, double tRad) {
        return tLength * (Math.pow(tRad,2) * Math.acos((tRad-inv)/tRad) - (tRad-inv)
        * Math.sqrt(2*tRad*inv-Math.pow(inv,2)));
    }//tankEq mathod brace

    //endregion

}//class brace
