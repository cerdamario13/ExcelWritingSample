//This program uses two arrays to calculate the inventory for Webster field
//Then, it writes the results in an excel file using Apache POI


import org.apache.poi.ss.usermodel.Cell;
import org.apache.poi.ss.usermodel.CellStyle;
import org.apache.poi.ss.usermodel.Row;
import org.apache.poi.xssf.usermodel.XSSFFont;
import org.apache.poi.xssf.usermodel.XSSFSheet;
import org.apache.poi.xssf.usermodel.XSSFWorkbook;

import java.io.File;
import java.io.FileOutputStream;
import java.util.ArrayList;
import java.util.Scanner;

public class WebsterInventory {
    public static void main(String[] args) {



        //Creating the excel workbook using Apache poi
        /*
            Creating one document with two sheets
         */
        XSSFWorkbook workbook = new XSSFWorkbook();
        //Creating the sheet and naming it
        XSSFSheet sheet = workbook.createSheet("Webster Inv");//For the inventory
        XSSFSheet sheet2 = workbook.createSheet("Deliveries");//For the deliveries


        //Creating the header row in the sheet. Using 0 base index
        Row header = sheet.createRow(0);
        //This is the reference used at the bottom when creating new cells and assigning values
        Row header2 = sheet2.createRow(0);//For the deliveries

        //Creating a style for the header
        CellStyle headerStyle = workbook.createCellStyle();
        //Adding a BOLD font to the header
        XSSFFont font = workbook.createFont();
        font.setFontName("Arial");//Naming the specific font to be used
        font.setBold(true); //Has to be boolean for BOLD font
        headerStyle.setFont(font);//Adding the 'font' to the 'headerStyle'

        //Creating the cell values in the header
        Cell headerCell = header.createCell(0);
        //Note that we have to reference 'header' in order to create a new cell and its value
        /* Adding the name of the columns
         * "WELL" "RATE" "Inventory" "DATE"  (Maybe later add "CURRENT RATE")
         * */

        //region Inventory headers
        headerCell.setCellValue("Location");
        headerCell.setCellStyle(headerStyle);//Calling the style that was set at "headerStyle" at top
        //Adding new columns header
        headerCell = header.createCell(1);//Second column
        headerCell.setCellValue("QPD");
        headerCell.setCellStyle(headerStyle);
        //Adding new column header
        headerCell = header.createCell(2);//Third column
        headerCell.setCellValue("Inventory");
        headerCell.setCellStyle(headerStyle);
        //Adding new column header
        headerCell = header.createCell(3);//Forth column
        headerCell.setCellValue("Days Left");
        headerCell.setCellStyle(headerStyle);
        //endregion

        //region Deliveries headers
        Cell headerCell2 = header2.createCell(0); //For deliveries //First Column
        headerCell2.setCellValue("Location");
        headerCell2.setCellStyle(headerStyle);
        headerCell2 = header2.createCell(1);//Second Column
        headerCell2.setCellValue("Delivery");
        headerCell2.setCellStyle(headerStyle);
        headerCell2 = header2.createCell(2); // Third Column
        //endregion


        /*Creating the array that will contain the well Numbers/Names. NAME: 'wellLoc#'
        the # corresponds to a gate number where the wells are located.
        Another array will be created for the pumping rates 'wellRate#'
         */
        // region GATE 3 wells and rates ArrayList
        ArrayList<String> wellLoc3 = new ArrayList<>();
        wellLoc3.add("178"); // #0
        wellLoc3.add("186"); // #1
        wellLoc3.add("167"); // #2
        //GATE 3 well rates
        ArrayList<Double> wellRate3 = new ArrayList<>(); //Do not use primitive data set. Use 'Double'
        wellRate3.add(12.0);
        wellRate3.add(8.0);
        wellRate3.add(8.0);
        //endregion

        //region GATE 4 wells and rates ArrayList
        ArrayList<String> wellLoc4 = new ArrayList<>(); //array for the wells
        ArrayList<Double> wellRate4 = new ArrayList<>(); //array for the rates
        wellLoc4.add("1831");wellRate4.add(9.0);
        wellLoc4.add("1835");wellRate4.add(10.0);
        wellLoc4.add("1830");wellRate4.add(4.0);
